{
  "server": {
    "request": "server/request.asp",
    "login": "POST server/login.asp",
    "logout": "server/logout.asp",
    "uploadValidation": "POST server/request.asp?FROM=%23panax.actualizarValidaciones",
    "uploadOVconfirmadas": "POST server/request.asp?FROM=%23panax.actualizarOVconfirmadas",
    "uploadStandardTimes": "POST server/request.asp?FROM=%23panax.actualizarTiempoEstandar",
    "uploadBlacklist": "POST server/request.asp?FROM=%23panax.actualizarPolizasIgnoradas",
    "applyPermissions": "POST server/request.asp?FROM=%23panax.applyPermissions"
  },
  "sources": {
    "^#(.+)": "assets/$1.xml",
    "#": "#shell",
    "#shell": null,
    "#login": null,
    "#settings": "settings.xml",
    "#menu": "menus.xml",
    "#sitemap": {
      "server:request": {
        "FROM": "#panax.getSitemap",
        "parameters": "'${xo.session.user_login}'"
      }
    },
    "#acceso_usuarios": {
      "server:request": {
        "FROM": "PanaxBI.acceso_usuarios"
      }
    }
  },
  "start": [ "loading.xslt" ],
  "settings": {
    "^#.*": {
      "headers": {
        "Cache-Control": "max-age=0",
        "x-debugging": "true",
        "expiry": 1
      }
    },
    "#estado_resultados_semanal": {
      "stylesheets": [
        {
          "href": "estado_resultados_semanal.xslt",
          "target": "@#shell main"
        }
      ]
    },
    "#inventario_mermas": {
      "headers": {
        "Cache-Control": "max-age=0",
        "expiry": 1
      }
    },
    "^#estado_resultados.*": {
      "^expiry": 1
    },
    "#topologia": {
      "^expiry": 1
    },
    "#acceso_usuarios": {
      "^expiry": 1
    },
    "#polizas": {
      "stylesheets": [
        {
          "href": "polizas.xslt",
          "target": "@#shell main"
        }
      ],
      "^expiry": 1
    },
    "server:applyPermissions": {
      "headers": {
        "x-payload-parameter-name": "@input"
      }
    },
    "#shell": {
      "hash": ""
    }
  }
}
